{
  "name": "vaultfy-clean",
  "version": "1.0.0",
  "license": "MIT",
  "engines": {
    "node": "20"
  },
  "scripts": {
    "postinstall": "yarn prisma:generate",
    "dev": "tsnd src/shared/infra/http/server.ts --exit-child --respawn --transpile-only --ignore-watch node_modules",
    "start": "node ./dist/shared/infra/http/server.js",
    "lint": "eslint src/**/*.ts --max-warnings=0",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "prisma:migrate": "prisma migrate dev --schema=./src/shared/infra/prisma/schema.prisma",
    "prisma:generate": "prisma generate --schema=./src/shared/infra/prisma/schema.prisma",
    "test": "jest"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.2",
    "reflect-metadata": "^0.2.1",
    "tsyringe": "^4.8.0",
    "yarn": "^1.22.21"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.7",
    "@babel/plugin-proposal-decorators": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-typescript": "^7.23.3",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.6",
    "@types/crypto-js": "^4.2.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.14.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "prettier": "^3.1.1",
    "prisma": "^5.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "*"
  }
}
