generator client {
  provider      = "prisma-client-js"
  binaryTargets = "linux-musl"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(cuid())
  username  String
  email     String   @unique
  password  String
  secret    String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
